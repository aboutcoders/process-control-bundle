{
    "name": "aboutcoders/process-control-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle that provides process control",
    "keywords": ["process", "control", "bundle", "symfony", "pcntl"],
    "homepage": "https://bitbucket.org/hasc/process-control-bundle",
    "license": "AboutCoders",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hannes.schulz@aboutcoders.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:hasc/process-control.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.1",
        "aboutcoders/process-control": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "symfony/monolog-bundle": "~2"
    },
    "autoload": {
        "psr-4": { "Abc\\Bundle\\ProcessControlBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}